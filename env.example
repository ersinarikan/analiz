# Flask Configuration
FLASK_ENV=development
FLASK_DEBUG=True
SECRET_KEY=your-secret-key-here

# Database Configuration
DATABASE_URL=sqlite:///wsanaliz.db
SQLALCHEMY_DATABASE_URI=sqlite:///wsanaliz.db
SQLALCHEMY_TRACK_MODIFICATIONS=False

# File Upload Configuration
UPLOAD_FOLDER=storage/uploads
MAX_CONTENT_LENGTH=104857600  # 100MB
ALLOWED_EXTENSIONS=jpg,jpeg,png,gif,mp4,avi,mov,mkv

# AI Models Configuration
MODELS_FOLDER=storage/models
USE_GPU=True
GPU_MEMORY_LIMIT=2048

# Age Model Configuration
AGE_MODEL_PATH=storage/models/age/custom_age_head/active_model/model.pth
AGE_MODEL_BASE_PATH=storage/models/age/custom_age_head/base_model
AGE_MODEL_ACTIVE_PATH=storage/models/age/custom_age_head/active_model
AGE_MODEL_VERSIONS_PATH=storage/models/age/custom_age_head/versions

# InsightFace Configuration
INSIGHTFACE_AGE_MODEL_BASE_PATH=storage/models/age/buffalo_l/base_model
INSIGHTFACE_AGE_MODEL_ACTIVE_PATH=storage/models/age/buffalo_l/active_model

# CLIP/OpenCLIP Configuration
OPENCLIP_MODEL_BASE_PATH=storage/models/clip/ViT-H-14-378-quickgelu_dfn5b
OPENCLIP_MODEL_ACTIVE_PATH=storage/models/clip/active_model
OPENCLIP_MODEL_VERSIONS_PATH=storage/models/clip/versions

# YOLO Detection Configuration
YOLO_MODEL_PATH=storage/models/detection/yolov8x/active_model/yolov8x.pt
DETECTION_MODEL_BASE_PATH=storage/models/detection/yolov8x/base_model
DETECTION_MODEL_ACTIVE_PATH=storage/models/detection/yolov8x/active_model

# Processing Configuration
BATCH_SIZE=32
MAX_FRAMES_PER_VIDEO=1000
FRAME_SAMPLING_RATE=30
THUMBNAIL_SIZE=150
FACE_DETECTION_CONFIDENCE=0.5

# Training Configuration
TRAINING_DATA_MIN_SAMPLES=10
CLEANUP_TRAINING_DATA_AFTER_TRAINING=True
AUTO_RESTART_AFTER_MODEL_CHANGE=True

# Cache Configuration
ENABLE_MODEL_CACHE=True
CACHE_TIMEOUT=3600

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=logs/wsanaliz.log

# Security Configuration
WTF_CSRF_ENABLED=True
WTF_CSRF_TIME_LIMIT=None

# Performance Configuration
THREADING=True
PROCESSES=1 